\name{catdist}
\alias{catdist}
\title{computing distances/dissimilarity for categorical variables}
\usage{
catdist(x,y=NULL,method="tot_var_dist")
}
\description{
Compute difference distance or dissimilarity indexes for categorical data
}
\arguments{
\item{x}{what is it}
\item{y}{optional}
\item{method}{distance of dissimilarity index of choice}
}
\value{
\item{distance matrix}{a distance matrix}
}
\examples{
hello()
}
