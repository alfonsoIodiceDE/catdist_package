\name{cat_nearest_neighbors}
\alias{cat_nearest_neighbors}
\title{Nearest neighbours classification for categorical data}
\usage{
cat_nearest_neighbors(train_df,assess_df = NULL, k, method = method)
}
\description{
computes K nearest neighbours classification for categorical data.
}
\arguments{
\item{train_df}{training set}
\item{assess_df}{assessment set}
}
\examples{
hello()
}
